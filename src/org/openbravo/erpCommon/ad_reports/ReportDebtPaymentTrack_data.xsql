    <Parameter name="adUserOrg" type="replace" optional="true" after="DP.AD_org_ID IN (" text="'7'"/>
    <Parameter name="cBpartnerId" optional="true" type="argument" after="and 7=7"><![CDATA[ and bp.c_bpartner_id in]]></Parameter>
