    <Parameter name="adUserOrg" type="replace" optional="true" after="Dp.AD_Org_ID IN (" text="'4'"/>  
    <Parameter name="cBpartnerId" optional="true" type="argument" after="and 4=4"><![CDATA[ and bp.c_bpartner_id in]]></Parameter>
