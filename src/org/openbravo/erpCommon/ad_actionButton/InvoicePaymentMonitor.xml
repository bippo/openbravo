<REPORT>
	<template file="InvoicePaymentMonitor.html"/>
	<PARAMETER id="paramLanguage" name="language" default=""/>
	<PARAMETER id="paramKey" name="key" default="" attribute="value"/>
	<PARAMETER id="window" name="window" default="" attribute="value"/>
	<PARAMETER id="tab" name="tab" default="" attribute="value"/>
	<PARAMETER id="question" name="question" default=""/>
	<PARAMETER id="paramDirectory" name="directory" default=""/>
	<PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
	<PARAMETER id="processDescription" name="description" replaceCharacters="htmlHelp" default=""/>
	<PARAMETER id="processHelp" name="help" replaceCharacters="htmlHelp" default=""/>
	<PARAMETER id="paramProcess" name="process" attribute="value" default=""/>
	<PARAMETER id="paramPath" name="path" attribute="value" default=""/>
	<PARAMETER id="paramTabName" name="tabname" attribute="value" default=""/>
	<PARAMETER id="linkButtonOk" name="ok" default="" attribute="onmouseover" replace="xx"/>
	<PARAMETER id="linkButtonCancel" name="cancel" default="" attribute="onmouseover" replace="xx"/>
	<PARAMETER id="buttonOk" name="ok" default="" attribute="alt" replace="xx"/>
	<PARAMETER id="buttonCancel" name="cancel" default="" attribute="alt" replace="xx"/>
	<PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"/>
	<PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""/>
	<PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""/>
	<DISCARD id="discard"/>
</REPORT>
