<REPORT>
  <template file="UsedByLink.html"/>
  <PARAMETER id="paramLanguage" name="language" default=""/>
  <PARAMETER id="paramDirectory" name="directory"/>
	<PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
  <PARAMETER id="fieldTabId" name="tabID" attribute="value"/>
  <PARAMETER id="fieldWindowId" name="windowID" attribute="value"/>
  <PARAMETER id="fieldKeyColumn" name="keyColumn" attribute="value"/>
  <PARAMETER id="fieldKeyId" name="keyName" attribute="name"/>
  <PARAMETER id="fieldKeyId" name="keyId" attribute="value"/>
  <PARAMETER id="fieldTableId" name="tableId" attribute="value"/>
  <PARAMETER id="fieldRecordIdentifier" name="recordIdentifier"/>
  
  <PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"></PARAMETER>
  <PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""></PARAMETER>
  <PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""></PARAMETER>

  <structure name="structure1">
    <FIELD id="fieldData" attribute="onclick" replace="xx">adTabId</FIELD>
    <FIELD id="fieldData" attribute="onclick" replace="yy">tablename</FIELD>
    <FIELD id="fieldData" attribute="onclick" replace="zz">columnname</FIELD>
    <FIELD id="fieldData" attribute="onmouseover" replace="xx">name</FIELD>
    <FIELD id="fieldElementName">elementName</FIELD>
    <FIELD id="fieldWindowName">windowname</FIELD>
    <FIELD id="fieldName">tabname</FIELD>
    <FIELD id="fieldTotal">total</FIELD>
    <SECTION id="fieldElementGroup" field="elementName"/>
    <SECTION id="sectionDetail"/>
  </structure>
  <DISCARD id="discard"/>
</REPORT>
